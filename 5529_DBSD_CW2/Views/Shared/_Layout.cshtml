                <div class="collapse navbar-collapse navbar-right" id="bs-example-navbar-collapse-1">
                    <nav class="menu menu--iris">
                     
                        <ul class="nav navbar-nav menu__list">
                            @if (Request.IsAuthenticated)
                            {
                                <li><span style="margin:10px">@Html.ActionLink(" Home ", "Index", "Home") </span></li>

                                <li><span style="margin:10px">@Html.ActionLink(" Flowers ", "Index", "Flower")</span></li>

                             

                                <li><span style="margin:10px">@Html.ActionLink(" Clients ", "Index", "Client")</span></li>
                                <li><span style="margin:10px">@Html.ActionLink(" Edit Profile ", "Edit", "Client", new { id = @Request.Cookies["userIdCook"].Value }, null)</span></li>
                                <li><span style="margin:10px">@Html.ActionLink(" WishList ", "Index", "Wishlist")</span></li>
                                <li><span style="margin:10px">@Html.ActionLink(" Report ", "Report", "Wishlist")</span></li>
                            }
                            @if (!Request.IsAuthenticated)
                            {
                                <li><span style="margin:10px">@Html.ActionLink(" Register Now ", "Create", "Client")</span></li>
                            }
                        </ul>


                    </nav>
                </div>
            </nav>

        </div>
